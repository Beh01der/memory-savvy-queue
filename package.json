{
  "name": "memory-savvy-queue",
  "version": "1.0.0",
  "description": "Simple FIFO queue that limits memory usage",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/Beh01der/memory-savvy-queue.git"
  },
  "scripts": {
    "build": "node build.js",
    "test": "jest",
    "test-dev": "jest --watch",
    "bump": "npm test && npm version patch && git push --tags && git push --all",
    "format": "dprint fmt ./src/**/*",
    "prepare": "npm test && npm run build"
  },
  "keywords": [
    "fifo",
    "queue",
    "memory",
    "node"
  ],
  "author": "Andrey Chausenko",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.17",
    "esbuild": "^0.18.14",
    "jest": "^29.5.0",
    "npm-dts": "^1.3.12",
    "object-sizeof": "^2.6.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  }
}
